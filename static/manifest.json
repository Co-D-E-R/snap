{
    "name": "ScreenShot Snap",
    "version": "0.1.0",
    "description": "Capture a ScreenShot of a video and Convert it to Pdf or download it as Image",
 
   
   
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "\u003Call_urls>",
      "*://*/*"
    ],
    "icons": {
      "128": "icons/icon128.png",
      "16": "icons/snap.png",
      "48": "icons/icon48.png",
      "256": "icons/snap2.png",
      "32": "icons/download.png"
    },
    "background": {
      "service_worker": "ServiceWorker.bundle.js"
    },
    "content_scripts": [
      {
        "all_frames": true,
        "match_about_blank": true,
        "matches": [
          "\u003Call_urls>"
        ],
        "js": [
          "contentScript.bundle.js"
        ],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "icons/icon128.png",
          "icons/snap.png",
          "icons/icon48.png",
          "icons/snap2.png",
          "icons/download.png",
          "icons/delete.png",
          "icons/setting.png"
        ],
        "matches": [
          "\u003Call_urls>"
        ]
      }
    ],
    "action": {
      "default_icon": {
        "16": "./icons/snap2.png",
        "24": "./icons/snap2.png",
        "32": "./icons/snap2.png"
      },
      "default_title": "Snapshort",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
  }